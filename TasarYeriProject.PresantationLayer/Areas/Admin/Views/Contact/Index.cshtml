@model List<Contact>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html>
<body>


    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h2>İletişim Mesajları</h2>
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="index.html">Ana Sayfa</a>
                </li>
                <li class="breadcrumb-item active">
                    <strong>Mesajlar</strong>
                </li>
            </ol>
        </div>
        <div class="col-lg-2">
        </div>
    </div>
    <div class="fh-breadcrumb">
        <div class="full-height">
            <div class="full-height-scroll">
                <ul class="list-group elements-list mb-2">
                    @foreach (var item in Model)
                    {
                        <li class="list-group-item">
                            <a class="nav-link">
                                <small class="float-right text-muted"> @item.ContactDate.ToShortDateString()</small>
                                <strong>@item.ContactSubject.ToUpper()</strong>
                                <div class="small m-t-xs">
                                    <p>
                                        @item.ContactMessage

                                    </p>
                                    <p class="m-b-none" >
                                        <i class="fa fa-user-o"> </i> @item.ContactUserName.ToUpper() / @item.ContactMail
                                    </p>
                                </div>
                            </a>
                        </li>

                    }
                    


                </ul>

            </div>
        </div>

    </div>






    <!-- Mainly scripts -->
    <script src="/AdminTema/js/jquery-3.1.1.min.js"></script>
    <script src="/AdminTema/js/popper.min.js"></script>
    <script src="/AdminTema/js/bootstrap.js"></script>
    <script src="/AdminTema/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/AdminTema/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="/AdminTema/js/inspinia.js"></script>
    <script src="/AdminTema/js/plugins/pace/pace.min.js"></script>


</body>

</html>
